#include <dt-bindings/sound/qcom,gpr.h>

&soc {
	qupv3_se3_spi: spi@98c000 {  /* BLSP3 QUP3*/
		status = "ok";
		qcom,helios-spi {
			compatible = "qcom,helios-spi";
			reg = <0>;
			spi-max-frequency = <180000>;
			interrupt-parent = <&tlmm>;
			qcom,irq-gpio = <&tlmm 30 1>;
		};
	};

	qcom,helios-daemon {
		compatible = "qcom,helios-daemon";
	};

};
